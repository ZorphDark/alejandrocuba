include ../setup/path

-
  var styles = [
    '//fonts.googleapis.com/css?family=Roboto|Roboto+Mono',
    '//cdnjs.cloudflare.com/ajax/libs/godlike.css/3.0.0/godlike.min.css',
    //- '//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css',
    path.css + "styles.min.css"
  ]

block styles
  for href in styles
    link(rel='stylesheet', href=href)
