include ../setup/path

-
  var scripts = [
    'https://cdnjs.cloudflare.com/ajax/libs/lightcase/2.3.4/js/lightcase.min.js',
    path.js + "scripts.min.js"
  ]

block scripts
  //- analytics
  script.
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-83815740-1', 'auto');
    ga('send', 'pageview');

  <!--[if lte IE 8]><script src="//code.jquery.com/jquery-1.12.4.min.js"></script><![endif]-->
  <!--[if gt IE 8]><!--><script src="//code.jquery.com/jquery-2.2.4.min.js"></script><!--<![endif]-->

  for href in scripts
    script(src=href async)
